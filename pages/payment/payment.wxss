.container{
  display: block;
  padding:0;
  width: 100%;
  height: auto;
}
.btns{
  float: right;
  margin-right: 40rpx;
  margin-top: 40rpx;
  height: 60rpx;
  line-height: 60rpx;
  width:160rpx;
  border:4rpx solid #2a8cff;
  text-align: center;
  border-radius: 8rpx;
}
.btns-text{
  font-size: 32rpx;
  color: #2a8cff;
}
.btns-cp{
  top: 0rpx;
  float: right;
  height: 80rpx;
  width: 160rpx;
}
.wallet{
  position: relative;
  top: 50rpx;
  left: 160rpx;
}
.images-wallet{
  height: 350rpx;
  width: 350rpx;
}
.texts{
  margin-top: 30rpx;
  text-align: center;
  font-size: 32rpx;
  color: #c4c4c4;
}
.btn-confir{
  margin-top: 180rpx;
  height: 90rpx;
  line-height: 90rpx;
  width: 80%;
  margin-left: auto;
  margin-right: auto;
  border-radius: 12rpx;
  text-align: center;
  background: #2a8cff;
}
.btn-text{
  font-family: sans-serif;
  font-size: 35rpx;
  color: #fff;
}


















